Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!Xcursor.theme: Amiga-Classic-Red
Xcursor.theme: El_Capitan_CursorsMODNew

!URxvt.font: xft: SF Mono:style=Light:size=12, \
!            xft: CozetteVector:style=Regular
!URxvt.font: xft: Gohu GohuFont:style=Regular:pixelsize=14

!URxvt.font: -*-terminus-medium-*-*--18-*-*-*-*-*-*-*, \
!            xft: CozetteVector:style=Regular

!URxvt.font: xft: Unifont:size=13, \
!            xft: CozetteVector:style=Regular

!URxvt.font: xft: Monaco:style=Regular:size=13, \
!            xft: CozetteVector:style=Regular

URxvt.font: xft: Comic Mono:style=Normal:size=14, \
            xft: Apple Color Emoji:style=Regular, \
            xft: CozetteVector:style=Regular

URxvt.letterSpace: -1
URxvt.depth:                      32
URxvt.geometry:                    140x42
URxvt.internalBorder:               40
!*.foreground:                     #d9d9d9
!*.background:                     #1C1C1C
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt.cursorBlink:                false
URxvt*saveLines:                  5000
URxvt*cursorColor:                #8f8a8d
!URxvt*fading:                     50
!URxvt*fadeColor:                  #657b83
URxvt*transparent:                false

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.C-v:     eval:paste_clipboard
URxvt.keysym.C-c:     eval:selection_to_clipboard
!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

